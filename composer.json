{
    "name": "spiderling-php/phpunit-spiderling",
    "description": "PHPUnit extension for seamlessly integrating spiderling",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ivan Kerin",
            "email": "ikerin@gmail.com",
            "role": "Author"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/spiderling-php/phantom-driver"
        }
    ],
    "require": {
        "spiderling-php/spiderling": "dev-master",
        "spiderling-php/phpunit-failure-action": "dev-master",
        "guzzlehttp/psr7": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "SP\\Phpunit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SP\\Phpunit\\Test\\": "tests/src/"
        }
    }
}
